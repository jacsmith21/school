gcc -g -Wall -Wshadow  -c matrix.c
gcc -g -Wall -Wshadow mymatrix.o matrix.o -o matrix
valgrind --tool=memcheck --leak-check=full --log-file=log0 ./matrix < input0 > output0
diff -q output0 expected0
valgrind --tool=memcheck --leak-check=full --log-file=log1 ./matrix < input1 > output1
diff -q output1 expected1
valgrind --tool=memcheck --leak-check=full --log-file=log2 ./matrix < input2 > output2
diff -q output2 expected2
valgrind --tool=memcheck --leak-check=full --log-file=log3 ./matrix < input3 > output3
diff -q output3 expected
